<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".CategoryActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="61dp">

    <TextView
        android:id="@+id/CategoryTitle"
        android:layout_width="360dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:padding="10dp"
        android:text="이 안에 네 최애 어쩌구 "
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Menu1"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="17dp"
        android:fontFamily="sans-serif-black"
        android:text="군옥수수"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toStartOf="@+id/Menu2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/corn" />

    <TextView
        android:id="@+id/Menu2"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="33dp"
        android:fontFamily="sans-serif-black"
        android:text="붕어빵"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toEndOf="@+id/Menu1"
        app:layout_constraintTop_toBottomOf="@+id/fish" />

    <TextView
        android:id="@+id/Menu3"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="35dp"
        android:fontFamily="sans-serif-black"
        android:text="떡볶이"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/red" />

    <TextView
        android:id="@+id/Menu4"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="30dp"
        android:fontFamily="sans-serif-black"
        android:text="오뎅"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ggochi" />

    <TextView
        android:id="@+id/Menu5"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="30dp"
        android:fontFamily="sans-serif-black"
        android:text="군고구마"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toEndOf="@+id/Menu4"
        app:layout_constraintTop_toBottomOf="@+id/sweet" />

    <TextView
        android:id="@+id/Menu6"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="35dp"
        android:fontFamily="sans-serif-black"
        android:text="토스트"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sand" />

    <TextView
        android:id="@+id/Menu7"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="34dp"
        android:fontFamily="sans-serif-black"
        android:text="타코야끼"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/octopus" />

    <TextView
        android:id="@+id/Menu8"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="34dp"
        android:fontFamily="sans-serif-black"
        android:text="와플"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintStart_toEndOf="@+id/Menu7"
        app:layout_constraintTop_toBottomOf="@+id/wa" />

    <TextView
        android:id="@+id/Menu9"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="35dp"
        android:fontFamily="sans-serif-black"
        android:text="닭꼬치"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sso" />

    <ImageView
        android:id="@+id/sweet"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toEndOf="@+id/ggochi"
        app:layout_constraintTop_toBottomOf="@+id/Menu2"
        app:srcCompat="@drawable/sweet" />

    <ImageView
        android:id="@+id/ggochi"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Menu1"
        app:srcCompat="@drawable/ggochi" />

    <ImageView
        android:id="@+id/sand"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toEndOf="@+id/sweet"
        app:layout_constraintTop_toBottomOf="@+id/Menu3"
        app:srcCompat="@drawable/sand" />

    <ImageView
        android:id="@+id/fish"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toEndOf="@+id/corn"
        app:layout_constraintTop_toBottomOf="@+id/CategoryTitle"
        app:srcCompat="@drawable/fish" />

    <ImageView
        android:id="@+id/corn"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CategoryTitle"
        app:srcCompat="@drawable/corn" />

    <ImageView
        android:id="@+id/red"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toEndOf="@+id/fish"
        app:layout_constraintTop_toBottomOf="@+id/CategoryTitle"
        app:srcCompat="@drawable/red" />

    <ImageView
        android:id="@+id/sso"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="31dp"
        app:layout_constraintStart_toEndOf="@+id/wa"
        app:layout_constraintTop_toBottomOf="@+id/Menu6"
        app:srcCompat="@drawable/sso" />

    <ImageView
        android:id="@+id/octopus"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="31dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Menu4"
        app:srcCompat="@drawable/takoyaki" />

    <ImageView
        android:id="@+id/wa"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="31dp"
        app:layout_constraintStart_toEndOf="@+id/octopus"
        app:layout_constraintTop_toBottomOf="@+id/Menu5"
        app:srcCompat="@drawable/wa" />

</androidx.constraintlayout.widget.ConstraintLayout>